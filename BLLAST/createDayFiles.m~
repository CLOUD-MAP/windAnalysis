%% Initialization

clear all

dirName = sprintf('.%sdata%s', filesep, filesep);
matFileNameIn = 'SUMO_BLLAST_DATA.mat';
load(matFileNameIn)

%% Processing

nFlights = length(sumo);
rdFlightDay = fix(sumo(1).time(1));
procFlightDay = fix(sumo(1).time(1));
matFileNameOut = sprintf('%sSUMOBLLAST.mat', datestr(procFlightDay), 'yyyymmdd');

for iFlight = 1: nFlights
    rdFlightDay = fix(sumo(iFlight).time);
    if rdFlag ~= procFlag
        save(matFileNameOut, 'sumoData');
        icnt = 1;
        sumoData = [];
        sumoData(icnt) = sumo;
    else
        icnt = icnt + 1;
        sumoData(icnt) = sumo;
    end
end
save(matFileNameOut, 'sumoData');